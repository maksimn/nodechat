{
    "env": {
        "browser": true,
        "jasmine": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jasmine/recommended"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "plugins": ["jasmine"],
    "rules": {
        "no-console": "off",
        "semi": 2
    }
}